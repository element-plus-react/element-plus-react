{
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:play": "pnpm -C playground/play run start",
    "start:docs": "dumi dev",
    "build:docs": "dumi build",
    "publish:docs": "gh-pages -d dist",
    "build": "cd packages/element-plus-react && rollup -c",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx && pretty-quick --branch dev"
  },
  "dependencies": {
    "dumi": "^1.1.48"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@element-plus/components": "workspace:1.0.0",
    "@element-plus/hooks": "workspace:1.0.0",
    "@element-plus/utils": "workspace:1.0.0",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-transition-group": "^4.4.5",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@umijs/plugin-sass": "^1.1.1",
    "cross-env": "^7.0.3",
    "element-plus-react": "workspace:0.0.4",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.3.4",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
