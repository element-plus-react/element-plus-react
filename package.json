{
  "name": "element-plus-react-docs",
  "private": true,
  "version": "0.0.1-alpha.0",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "start:play": "yarn workspace play run start",
    "start:docs": "dumi dev",
    "build:docs": "dumi build",
    "publish:docs": "gh-pages -d dist",
    "build": "yarn workspace element-plus-react run build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx && pretty-quick --branch dev"
  },
  "bugs": {
    "url": "https://github.com/element-plus-react/element-plus-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/element-plus-react/element-plus-react"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@ctrl/tinycolor": "^3.4.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
