{
  "name": "element-plus-react",
  "version": "0.0.1-alpha.8",
  "homepage": "https://element-plus-react.github.io/",
  "scripts": {
    "start:play": "cd play && yarn run start",
    "start:docs": "dumi dev",
    "build:docs": "dumi build",
    "publish:docs": "gh-pages -d dist",
    "build": "rollup -c",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx && pretty-quick --branch dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/element-plus-react/element-plus-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/element-plus-react/element-plus-react"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./es": "./es/index.js",
    "./lib": "./lib/index.js",
    "./*": "./*"
  },
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "dist/index.css",
  "types": "es/index.d.ts",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ctrl/tinycolor": "^3.4.0",
    "ahooks": "^3.1.7",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@ctrl/tinycolor": "^3.4.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@umijs/plugin-sass": "^1.1.1",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "dumi": "^1.1.38",
    "element-plus": "^1.3.0-beta.5",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^25.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^40.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.66.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "^1.47.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
