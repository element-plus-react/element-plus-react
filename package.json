    "eslint-plugin-unicorn": "^40.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-postcss": "^4.0.2",
    "postcss": "8",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.47.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
