{
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "prepare": "husky install && dumi setup",
    "start:play": "pnpm -C playground/play run start",
    "start:docs": "dumi dev",
    "build:docs": "dumi build",
    "publish:docs": "gh-pages -d dist",
    "build": "cd packages/element-plus-react && rollup -c --bundleConfigAsCjs",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx && pretty-quick --branch dev"
  },
  "dependencies": {
    "dumi": "^2.0.0-rc.6"
  },
  "devDependencies": {
    "@element-plus/components": "workspace:1.0.0",
    "@element-plus/eslint-config": "workspace:1.0.0",
    "@element-plus/hooks": "workspace:1.0.0",
    "@element-plus/theme-chalk": "workspace:1.0.0",
    "@element-plus/utils": "workspace:1.0.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "cross-env": "^7.0.3",
    "element-plus-react": "workspace:0.0.4",
    "eslint": "^8.27.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
