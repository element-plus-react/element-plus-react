    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@umijs/plugin-sass": "^1.1.1",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^8",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "dumi": "^1.1.38",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-babel": "^5.3.0",
