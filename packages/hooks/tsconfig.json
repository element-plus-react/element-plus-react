{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {

    },
    "strictNullChecks": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true,
    "rootDir": "../",
    "declaration": true,
    "declarationDir": "./types",
    "outDir": "./",
    "sourceMap": true
  },
  "include": [
    "./",
    "../",
  ],
  "exclude": ["node_modules", "lib", "es"]
}
