{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "element-plus-react": ["src/index.ts"]
    },
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true,
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "typings",
    "outDir": "./",
    "sourceMap": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib", "es"]
}
