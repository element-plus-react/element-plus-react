{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react-jsx",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "typings",
    "outDir": "./",
    "sourceMap": true,
    "typeRoots": ["./typings"],
    "preserveSymlinks": true,
    "paths": {
      "element-plus-react": ["./internal/element-plus-react/src/index.ts"],
      "@element-plus/components": ["./packages/components/src/index.ts"],
      "@element-plus/utils": ["./packages/utils/src/index.ts"],
      "@element-plus/hooks": ["./packages/hooks/src/index.ts"]
    },
    "types": ["@types/node"]
  },
  "include": [".", ".dumirc.ts"],
  "exclude": ["node_modules", "lib", "es"]
}
